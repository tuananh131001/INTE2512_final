   <BorderPane fx:id="borderPane">
      <top>
         <MenuBar fx:id="menuBar" prefHeight="32.0" prefWidth="710.0" BorderPane.alignment="CENTER">
            <effect>
               <Glow />
            </effect></MenuBar>
      </top>
      <center>
         <ScrollPane fx:id="scrollPaneFilters" fitToHeight="true" fitToWidth="true" pannable="true" stylesheets="@styles/scrollstyle.css">
            <Pagination fx:id="page" BorderPane.alignment="BOTTOM_CENTER" />
         </ScrollPane>
      </center>
      <bottom>

      </bottom>
   </BorderPane>
</StackPane>
